<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converters="clr-namespace:Omnipaste.Framework.Converters"
                    xmlns:converters1="clr-namespace:OmniUI.Converters;assembly=OmniUI"
                    xmlns:converters2="clr-namespace:Omnipaste.Activity.Converters">
    <converters:LimitedStringConverter x:Key="LimitedStringConverter" />
    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
    <converters1:InverseBooleanToVisibilityConverter x:Key="InverseBooleanToVisibilityConverter" />
    <converters:HumanizedStringConverter x:Key="HumanizedStringConverter"/>
    <converters1:StringToResourceConverter x:Key="StringToResourceConverter"/>
    <converters:StringNullOrEmptyToVisibilityConverter x:Key="StringNullOrEmptyToVisibilityConverter"/>
    <converters:InverseStringNullOrEmptyToVisibilityConverter x:Key="InverseStringNullOrEmptyToVisibilityConverter"/>
    <converters:InvertedBooleanToVisibilityConverter x:Key="InvertedBooleanToVisibilityConverter"/>
    <converters:NonWhiteSpaceStringConverter x:Key="NonWhiteSpaceStringConverter"/>
    <converters2:ViewedSizeConverter x:Key="ViewedSizeConverter"/>
    <converters1:StringToUriConverter x:Key="StringToUriConverter"/>
</ResourceDictionary>