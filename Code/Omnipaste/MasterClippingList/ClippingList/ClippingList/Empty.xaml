<UserControl x:Class="Omnipaste.MasterClippingList.ClippingList.ClippingList.Empty"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:p="clr-namespace:Omnipaste.Properties"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <StackPanel Style="{DynamicResource EmptyClippingsContainer}">
            <Rectangle Style="{DynamicResource EmptyListIconStyle}" />
            <TextBlock Text="{x:Static p:Resources.ClippingListEmptyLabel}" Style="{StaticResource InfoTextStyleCenter}" />
            <TextBlock Text="{x:Static p:Resources.ClippingListEmptyContent}" Style="{StaticResource PlaceholderContentStyle}" />
        </StackPanel>
    </Grid>
</UserControl>
